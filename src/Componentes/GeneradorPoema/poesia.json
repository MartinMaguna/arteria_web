{
    "poema": [
      {
        "versos": ["No elegí migrar,", "me llevó el azar."]
      },
      {
        "versos": ["Pensé que era para siempre.", "Pensé que era por 8 años."]
      },
      {
        "versos": ["Siempre me he puesto tangencial."]
      },
      {
        "versos": ["Mi mayor migración."]
      },
      {
        "versos": ["Deje de amar,", "me lleve amor…", "me perdí en mi."]
      },
      {
        "versos": ["El peligro de perderse.", "A veces todo, a veces nada."]
      },
      {
        "versos": [
          "Hoy soy en Uruguay",
          "Brasil, Argentina,",
          "Chile, Cuba",
          "República Dominicana,",
          "en México, en Málaga."
        ]
      },
      {
        "versos": ["Perdí la inocencia.", "He sentido el rechazo.", "Extraño la ligereza cotidiana."]
      },
      {
        "versos": ["El riesgo es el punto de no retorno,", "cuando se traspasa una frontera."]
      },
      {
        "versos": ["El río.", "El puente.", "El telégrafo."]
      },
      {
        "versos": ["Y los partos, la edad, la familia."]
      },
      {
        "versos": ["Nací donde nací."]
      },
      {
        "versos": ["Me llevé varios dolores", "guardados."]
      },
      {
        "versos": ["Volvería", "Donde están mis afectos"]
      },
      {
        "versos": ["Una migración", "radical y definitiva."]
      },
      {
        "versos": [
          "Nací por casualidad",
          "y pienso en exilios",
          "políticos, económicos",
          "o climáticos."
        ]
      },
      {
        "versos": ["Por refugio.", "De un país a otro."]
      },
      {
        "versos": [
          "Aprendí que rico,",
          "no es el q más tiene, sino,",
          "el que menos necesita."
        ]
      },
      {
        "versos": ["Extraño", "la cara", "de la gente"]
      },
      {
        "versos": [
          "Pienso en migrar como",
          "Un movimiento de animales",
          "que incluye al homo sapiens."
        ]
      },
      {
        "versos": ["Tu cabeza, tu vida, tu tiempo."]
      },
      {
        "versos": ["El miedo era la tensión.", "Aunque me muera de miedo."]
      },
      {
        "versos": ["Perdí mi cuerpo abandonado"]
      },
      {
        "versos": ["El dolor fue productivo y liberador."]
      },
      {
        "versos": ["El riesgo del migrante."]
      },
      {
        "versos": ["Mi mayor migración,", "de agnóstico a hijo de Dios."]
      },
      {
        "versos": ["Pienso en migración como conversión."]
      },
      {
        "versos": ["Hoy me sigo despojando de mis ataduras."]
      },
      {
        "versos": ["Hoy elijo mi plasticidad."]
      },
      {
        "versos": ["Migrar como un movimiento."]
      },
      {
        "versos": ["Se mudan a otra parte, lejana."]
      },
      {
        "versos": ["Me gustaría", "nunca dejar de migrar."]
      }
    ]
  }
  